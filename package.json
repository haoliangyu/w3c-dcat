{
  "name": "w3c-dcat",
  "version": "0.2.0",
  "description": "A utility library to create and convert W3C DCAT in JSON",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "mocha test/**/*.test.ts --require ts-node/register",
    "doc": "typedoc --theme minimal --name 'w3c-dcat' --out docs --readme README.md --includeDeclarations --excludeExternals src",
    "build": "tsc src/index.ts --outDir dist --declaration && cpx 'src/dcat/*' dist/dcat",
    "precommit": "tslint src && npm run build && npm test && lint-staged"
  },
  "lint-staged": {
    "src/*.{d.ts,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haoliangyu/w3c-dcat.git"
  },
  "author": "Haoliang Yu <haholiang.yu@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haoliangyu/w3c-dcat/issues"
  },
  "keywords": [
    "w3c",
    "dcat",
    "utility-library",
    "json",
    "interoperation"
  ],
  "homepage": "https://github.com/haoliangyu/w3c-dcat#readme",
  "devDependencies": {
    "@types/chai": "^4.0.7",
    "@types/mocha": "^2.2.44",
    "@types/node-fetch": "^1.6.7",
    "chai": "^4.1.2",
    "cpx": "^1.5.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "mocha": "^4.0.1",
    "node-fetch": "^1.7.3",
    "prettier": "^1.9.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@types/geojson": "^7946.0.0",
    "franc-min": "^3.1.1",
    "lodash": "^4.17.4",
    "wkx": "^0.4.2"
  }
}

  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haoliangyu/w3c-dcat/issues"
  },
  "keywords": [
    "w3c",
    "dcat",
    "utility-library",
    "json"
  ],
  "homepage": "https://github.com/haoliangyu/w3c-dcat#readme",
  "devDependencies": {
    "@types/chai": "^4.0.7",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "cpx": "^1.5.0",
    "husky": "^0.14.3",
    "mocha": "^4.0.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@types/geojson": "^7946.0.0",
    "lodash": "^4.17.4",
    "wkx": "^0.4.2"
  }

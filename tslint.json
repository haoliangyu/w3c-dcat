{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      false
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "member-ordering": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "max-line-length": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "no-console": [
      false
    ]
  }
}
